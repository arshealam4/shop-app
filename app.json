{
  "expo": {
    "name": "Shop App",
    "slug": "shop-app",
    "description": "Graet App to shop",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.shop.great.app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.shop.great.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#171717"
      }
    }
  }
}
